<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="YiChing.CvConfig">

    <VerticalStackLayout>
        <Label Text="Answer Language" />
        <Entry Text="{Binding Settings.AnswerLanguage}" />
        <Label Text="Key Two" />
        <Entry Text="{Binding Settings.KeyTwo}" />
        <Label Text="Key Three Message" />
        <Entry Text="{Binding Settings.KeyThree.Message}" />
        <Button x:Name="btnReturn" Text="Return"></Button>
    </VerticalStackLayout>
</ContentView>
